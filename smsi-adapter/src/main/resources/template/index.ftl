					url += "&port=" + webservicePort;
				}
				
				//document.getElementById("op_A").href = url;
				//document.getElementById("op_A").click();
				
				jQuery.ajax({
					url:url,
					dataType:"text",
					async:false,
					cache:false,
					type:"GET",
					success:function(data){
						if(data == "success"){
							location.href = "/";
						}else{
							alert(data);
						}
					}
				});
			}
		</script>
